---
# Create a schema template with a single table in it (and no indexes). This allows us to check the behavior
# of a scan operator with versions, in the case where there is no type filter above it, something that
# should only happen when there are no other types
schema_template:
    create table t1(id bigint, col1 bigint, col2 bigint, primary key(id))

    create function t1_v(in x bigint) as select "__ROW_VERSION", id, col1, col2 from t1 where col1 = x

    with options (store_row_versions=true)
---
setup:
  steps:
    # Insert the odd rows first
    - query: INSERT INTO T1(ID, COL1, COL2) VALUES
         (1, 10, 1),
         (3, 10, 3),
         (5, 10, 5),
         (7, 20, 7),
         (9, 20, 9),
         (11, 20, 11),
         (13, 20, 13)
    # Then insert the even rows so that they are given higher row versions
    - query: INSERT INTO T1(ID, COL1, COL2) VALUES
         (2, 10, 2),
         (4, 10, 4),
         (6, 20, 6),
         (8, 20, 8),
         (10, 20, 10),
         (12, 20, 12)
---
test_block:
  name: single-type-versions-queries
  tests:
    -
      - query: select * from t1;
      - explain: "SCAN(<,>) | MAP (_.ID AS ID, _.COL1 AS COL1, _.COL2 AS COL2)"
      - unorderedResult: [
          { ID:  1, COL1: 10, COL2:  1 },
          { ID:  2, COL1: 10, COL2:  2 },
          { ID:  3, COL1: 10, COL2:  3 },
          { ID:  4, COL1: 10, COL2:  4 },
          { ID:  5, COL1: 10, COL2:  5 },
          { ID:  6, COL1: 20, COL2:  6 },
          { ID:  7, COL1: 20, COL2:  7 },
          { ID:  8, COL1: 20, COL2:  8 },
          { ID:  9, COL1: 20, COL2:  9 },
          { ID: 10, COL1: 20, COL2: 10 },
          { ID: 11, COL1: 20, COL2: 11 },
          { ID: 12, COL1: 20, COL2: 12 },
          { ID: 13, COL1: 20, COL2: 13 }
        ]
    -
      - query: select *, "__ROW_VERSION" from t1;
      - explain: "SCAN(<,>) | MAP (_.ID AS ID, _.COL1 AS COL1, _.COL2 AS COL2, version([_]) AS __ROW_VERSION)"
      - unorderedResult: [
          { ID:  1, COL1: 10, COL2:  1, "__ROW_VERSION": !not_null _ },
          { ID:  2, COL1: 10, COL2:  2, "__ROW_VERSION": !not_null _ },
          { ID:  3, COL1: 10, COL2:  3, "__ROW_VERSION": !not_null _ },
          { ID:  4, COL1: 10, COL2:  4, "__ROW_VERSION": !not_null _ },
          { ID:  5, COL1: 10, COL2:  5, "__ROW_VERSION": !not_null _ },
          { ID:  6, COL1: 20, COL2:  6, "__ROW_VERSION": !not_null _ },
          { ID:  7, COL1: 20, COL2:  7, "__ROW_VERSION": !not_null _ },
          { ID:  8, COL1: 20, COL2:  8, "__ROW_VERSION": !not_null _ },
          { ID:  9, COL1: 20, COL2:  9, "__ROW_VERSION": !not_null _ },
          { ID: 10, COL1: 20, COL2: 10, "__ROW_VERSION": !not_null _ },
          { ID: 11, COL1: 20, COL2: 11, "__ROW_VERSION": !not_null _ },
          { ID: 12, COL1: 20, COL2: 12, "__ROW_VERSION": !not_null _ },
          { ID: 13, COL1: 20, COL2: 13, "__ROW_VERSION": !not_null _ }
        ]
    -
      - query: select "__ROW_VERSION", * from t1 where col1 = 20;
      - explain: "SCAN(<,>) | FILTER _.COL1 EQUALS promote(@c10 AS LONG) | MAP (version([_]) AS __ROW_VERSION, _.ID AS ID, _.COL1 AS COL1, _.COL2 AS COL2)"
      - unorderedResult: [
          { "__ROW_VERSION": !not_null _, ID:  6, COL1: 20, COL2:  6 },
          { "__ROW_VERSION": !not_null _, ID:  7, COL1: 20, COL2:  7 },
          { "__ROW_VERSION": !not_null _, ID:  8, COL1: 20, COL2:  8 },
          { "__ROW_VERSION": !not_null _, ID:  9, COL1: 20, COL2:  9 },
          { "__ROW_VERSION": !not_null _, ID: 10, COL1: 20, COL2: 10 },
          { "__ROW_VERSION": !not_null _, ID: 11, COL1: 20, COL2: 11 },
          { "__ROW_VERSION": !not_null _, ID: 12, COL1: 20, COL2: 12 },
          { "__ROW_VERSION": !not_null _, ID: 13, COL1: 20, COL2: 13 }
        ]
    -
      - query: select * from t1_v(10)
      - explain: "SCAN(<,>) | FILTER _.COL1 EQUALS promote(@c6 AS LONG) | MAP (version([_]) AS __ROW_VERSION, _.ID AS ID, _.COL1 AS COL1, _.COL2 AS COL2)"
      - unorderedResult: [
          { "__ROW_VERSION": !not_null _, ID:  1, COL1: 10, COL2:  1 },
          { "__ROW_VERSION": !not_null _, ID:  2, COL1: 10, COL2:  2 },
          { "__ROW_VERSION": !not_null _, ID:  3, COL1: 10, COL2:  3 },
          { "__ROW_VERSION": !not_null _, ID:  4, COL1: 10, COL2:  4 },
          { "__ROW_VERSION": !not_null _, ID:  5, COL1: 10, COL2:  5 },
        ]
...
