---
schema_template:
    create table t1(c1 bigint, c2 string, c3 bigint, primary key(c1))
---
test_block:
  preset: single_repetition_ordered
  tests:
    -
      - query: insert into t1 values (10, 'test 20', 30),
                                     (11, 'test 21', null)
    -
      - query: select * from t1
      - unorderedResult: [{!pos 1: 10, !pos 2: 'test 20', !pos 3: !ignore _ },
                          {!pos 1: 11, !pos 2: 'test 21', !pos 3: !null }]
    -
      - query: select * from t1 where c1 = 10
      - unorderedResult: [{!pos 2: 'test 20', !pos 3: 30, !pos 1: 10}]