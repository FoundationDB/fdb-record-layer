---
schema_template:
    CREATE TYPE AS STRUCT s1(a bigint, b bigint, q bigint, z bigint)
    create table t1(id s1, g bigint, primary key(id.a, id.b))
---
test_block:
  name: primary-key-tests
  preset: single_repetition_ordered
  tests:
    -
      - query: INSERT INTO T1
           VALUES ((1, 2, 3, 4), 5),
                  ((1, 2, 30, 40), 50)
      - error: "23505"
    # 4.1 Triage: [Deferred: Not failing in CK] failed when running with forced continuations against 4.0.559.6 (Received continuation shouldn't be at beginning)
    -
      - query: SELECT COUNT(*) FROM T1
      - maxRows: 0
      - result: [{0}]
    -
      - query: INSERT INTO T1
          VALUES ((1, 2, 3, 4), 5),
                 ((1, 3, 30, 40), 50)
      - count: 2
    -
      - query: select * from t1;
      - result: [{{1, 2, 3, 4}, 5},
                 {{1, 3, 30, 40}, 50}]
...
